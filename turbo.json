{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*", "!**/*.stories.{tsx,jsx,mdx}"],
			"outputs": [".next/**", "!.next/cache/**", "storybook-static/**"]
		},
		"build:web": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*", "!**/*.stories.{tsx,jsx,mdx}"],
			"outputs": [".next/**", "!.next/cache/**", "storybook-static/**"]
		},
		"build:storybook": {},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
